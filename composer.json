{
    "name": "cencerro/omnipay-pagandocheck",
    "description": "Pasarela de pago Pagando Check para OmniPay.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Pagando Check",
        "pagando",
        "check",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "authors": [
        {
            "name": "lmaldonado-cencerro",
            "email": "ferleon@cencerro.com.mx"
        }
    ],
    "homepage": "https://github.com/CencerroMX/Cencerro-Omnipay-PagandoCheck",
    "autoload": {
        "psr-4": {
            "Cencerro\\OmnipayPagandoCheck\\": "src/"
        }
    },
    "require": {
        "league/omnipay": "^3.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
